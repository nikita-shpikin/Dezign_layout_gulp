header.header
	.container.header-container
		a.header__logo(href="#")
			img(src=`./logo.svg`, alt=`logo`)
		menu.header-list
			each value in ['HOME', 'Work', 'About']
				li
					if(value === 'HOME')
						a.header__link.header__link_active(href="#") #{value}
					if(value === 'Work')
						a.header__link(href=`#${value}`) #{value}
					if(value === 'About')
						a.header__link(href=`#${value}`) #{value}